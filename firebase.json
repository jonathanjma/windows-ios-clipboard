{
  "hosting": {
    "site": "win-ios-clipboard",
    "public": "firebase",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/.idea/**",
      "**/python/**"
    ],
    "rewrites": [
      {
        "source": "/api",
        "function": "api"
      },
      {
        "source": "/paste",
        "function": "api"
      },
      {
        "source": "/copy",
        "function": "api"
      }
    ]
  }
}
